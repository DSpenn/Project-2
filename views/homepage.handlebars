
<div class="table-responsive-xl d-grid gap-5">
<table class="table table-striped table-dark table-hover">
  <thead class="thead-dark">
    <tr>
      <th scope="col-xl">Workout Name</th> 
      <th scope="col">excercise</th>
      <th scope="col-sm">sets</th>
      <th scope="col-sm">reps</th>
    </tr>
  </thead>
     {{#each workouts as |workout| }}
  <tbody class="">
    <tr class="">
      <th class="row" scope="row"><div class="col-9">{{workout.workout_name}}</div>
      <div class="col-4">{{this.user.username}}</div></th>
      <td>{{workout.excercise}}</td>
      <td>{{workout.sets}}</td>
      <td>{{workout.reps}}</td>
    </tr>
  </tbody>
  {{/each}}
</table>
</div>


<h2>User List</h2>
{{#each workouts as |workout| }}
<div class="row align-center mb-1">
  <div class="col-md-6">
    <p>{{this.user.username}}</p>
  </div>
  <div class="col-md-6">
  </div>
</div>
{{/each}}